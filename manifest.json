{
  "manifest_version": 3,
  "name": "LinkedIn Profile Data Extractor",
  "version": "2.0",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://www.linkedin.com/in/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabGroups",
    "tabs",
    "storage",
    "activeTab"
  ]
}